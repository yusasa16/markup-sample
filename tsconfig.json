{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@styles/*": ["./src/styles/*"],
			"@pages/*": ["./src/pages/*"],
			"@utils/*": ["./src/utils/*"]
		}
	}
}
